{
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "index.js"
    },
    {
      "src": "/.*",
      "dest": "cron.js"
    }
  ],
  "crons": [
    {
      "path": "/cron",
      "schedule": "30 19 * * *"
    }
  ]
}
